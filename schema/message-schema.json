{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "message-schema.json",
    "title": "Message",
    "description": "Message format of the ViPLab Websocket API",
    "type": "object",
    "properties": {
        "type": {
            "type": "string",
            "description": "The type of the message"
        },
        "content": {
            "type": "object",
            "description": "The content of the message"
        }
    },
    "required": [
        "type",
        "content"
    ]
}
